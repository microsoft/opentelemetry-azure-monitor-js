{
  "name": "@azure/opentelemetry-exporter",
  "version": "0.0.0",
  "description": "Application Insights exporter for the OpenTelemetry JavaScript (Node.js) SDK",
  "main": "build/src/index.js",
  "private": true,
  "scripts": {
    "clean": "rimraf build",
    "build": "tsc -p ./tsconfig.json",
    "lint": "eslint . --ext .ts",
    "test": "ts-mocha -p ./tsconfig.json 'test/**/*.test.ts'",
    "report": "c8 report --reporter=json"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "author": "appinsightssdk@microsoft.com",
  "license": "MIT",
  "dependencies": {
    "@opentelemetry/base": "^0.7.0",
    "@opentelemetry/core": "^0.7.0",
    "@opentelemetry/tracing": "^0.7.0",
    "@opentelemetry/api": "^0.7.0"
  }
}
